{
  "applications": {
    "gecko": {
      "id": "sidebarTabs@asamuzak.jp",
      "strict_min_version": "64.0a1"
    }
  },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "img/icon.svg#context",
    "default_title": "__MSG_extensionName__"
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/sidebarTabs",
  "icons": {
    "32": "img/icon.svg#current",
    "64": "img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "options_ui": {
    "browser_style": false,
    "open_in_tab": true,
    "page": "html/options.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "contextualIdentities",
    "cookies",
    "management",
    "menus",
    "menus.overrideContext",
    "sessions",
    "storage",
    "tabs"
  ],
  "short_name": "__MSG_extensionShortName__",
  "sidebar_action": {
    "browser_style": false,
    "default_icon": "img/icon.svg#context",
    "default_panel": "html/sidebar.html",
    "default_title": "__MSG_extensionShortName__"
  },
  "version": "1.2.0a1"
}
