{
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "img/icon.svg#current",
    "default_title": "__MSG_extensionName__",
    "theme_icons": [
      {
        "dark": "img/icon.svg#dark",
        "light": "img/icon.svg#light",
        "size": 16
      },
      {
        "dark": "img/icon.svg#dark",
        "light": "img/icon.svg#light",
        "size": 32
      }
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "sidebarTabs@asamuzak.jp",
      "strict_min_version": "89.0a1"
    }
  },
  "commands": {
    "toggleOpenCloseState": {
      "description": "__MSG_toggleOpenCloseState__"
    }
  },
  "default_locale": "en",
  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/asamuzaK/sidebarTabs",
  "icons": {
    "32": "img/icon.svg#current",
    "64": "img/icon.svg"
  },
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "optional_permissions": [
    "browserSettings"
  ],
  "options_ui": {
    "browser_style": true,
    "open_in_tab": true,
    "page": "html/options.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "contextualIdentities",
    "cookies",
    "management",
    "menus",
    "menus.overrideContext",
    "search",
    "sessions",
    "storage",
    "tabs",
    "theme"
  ],
  "short_name": "__MSG_extensionShortName__",
  "sidebar_action": {
    "browser_style": false,
    "default_icon": "img/icon.svg#current",
    "default_panel": "html/sidebar.html",
    "default_title": "__MSG_extensionShortName__"
  },
  "version": "7.1.0"
}
