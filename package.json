{
  "name": "sidebartabs",
  "description": "Emulate tabs in sidebar.",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/sidebarTabs",
  "bugs": {
    "url": "https://github.com/asamuzaK/sidebarTabs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asamuzaK/sidebarTabs.git"
  },
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "include": "node index include -i",
    "include:tldts": "node index include -d tldts -i",
    "lint": "npm-run-all -s lint:*",
    "lint:addons-linter": "addons-linter src",
    "lint:eslint": "eslint --fix .",
    "lint:html": "html-validate src/html/*.html",
    "lint:style": "stylelint --fix src/css/*.css",
    "test": "npm run test:central",
    "test:beta": "c8 mocha test/*.test.js --require test/mocha/setup.js --channel=beta --exit",
    "test:central": "c8 mocha test/*.test.js --require test/mocha/setup.js --channel=central --exit",
    "test:esr": "c8 mocha test/*.test.js --require test/mocha/setup.js --channel=esr --exit",
    "test:release": "c8 mocha test/*.test.js --require test/mocha/setup.js --channel=release --exit",
    "test_all": "npm-run-all -s test:*",
    "update": "node index update -i",
    "update:alpen": "node index update -d alpenglow -i",
    "update:dark": "node index update -d dark -i",
    "update:light": "node index update -d light -i"
  },
  "dependencies": {
    "camelize": "^1.0.0",
    "commander": "^8.3.0",
    "copyfiles": "^2.4.1",
    "decamelize": "^6.0.0",
    "node-fetch": "^3.2.0",
    "tldts-experimental": "^5.7.59"
  },
  "devDependencies": {
    "addons-linter": "^4.6.0",
    "c8": "^7.11.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.6.3",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "html-validate": "^6.2.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.4",
    "nock": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^12.0.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "webext-schema": "^2.0.6"
  },
  "version": "6.4.0"
}
