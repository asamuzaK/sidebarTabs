{
  "name": "sidebartabs",
  "description": "Emulate tabs in sidebar.",
  "author": "asamuzaK",
  "homepage": "https://github.com/asamuzaK/sidebarTabs",
  "bugs": {
    "url": "https://github.com/asamuzaK/sidebarTabs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asamuzaK/sidebarTabs.git"
  },
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "lint": "npm-run-all -s lint:*",
    "lint:addons-linter": "addons-linter src",
    "lint:eslint": "eslint --fix .",
    "lint:html": "html-validate src/html/*.html",
    "lint:style": "stylelint --fix src/css/*.css",
    "test": "npm run test:central -- --experimental-json-modules",
    "test:beta": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=beta --exit -- --experimental-json-modules",
    "test:central": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=central --exit -- --experimental-json-modules",
    "test:esr": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=esr --exit -- --experimental-json-modules",
    "test:release": "c8 --reporter=text mocha test/*.test.js --require test/mocha/setup.js --channel=release --exit -- --experimental-json-modules",
    "test_all": "npm-run-all -s test:*",
    "update": "node index update -i",
    "update:alpen": "node index update -d alpenglow -i",
    "update:dark": "node index update -d dark -i",
    "update:light": "node index update -d light -i"
  },
  "dependencies": {
    "camelize": "^1.0.0",
    "commander": "^8.3.0",
    "decamelize": "^6.0.0",
    "node-fetch": "^3.1.0"
  },
  "devDependencies": {
    "addons-linter": "^4.2.0",
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-no-unsanitized": "^4.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "html-validate": "^6.1.3",
    "jsdom": "^18.1.1",
    "mocha": "^9.1.3",
    "nock": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "psl": "^1.8.0",
    "sinon": "^12.0.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "webext-schema": "^2.0.3"
  },
  "version": "6.3.0"
}
